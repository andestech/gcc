		 riscv_v_adjust_nunits (RVVM##LMUL##DImode, false, LMUL, 1));  \
  ADJUST_NUNITS (RVVM##LMUL##HF,                                               \
		 riscv_v_adjust_nunits (RVVM##LMUL##HFmode, false, LMUL, 1));  \

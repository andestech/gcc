  VECTOR_MODE_WITH_PREFIX (RVVM, INT, HI, LMUL, 0);                            \
  VECTOR_MODE_WITH_PREFIX (RVVM, FLOAT, HF, LMUL, 0);                          \
  VECTOR_MODE_WITH_PREFIX (RVVM, FLOAT, BF, LMUL, 0);                          \

/* Invoke DEF_RISCV_FTYPE (NARGS, LIST) for each prototype used by
   RISCV built-in functions, where:

      NARGS is the number of arguments.
      LIST contains the return-type code followed by the codes for each
        argument type.  */

DEF_RISCV_FTYPE (0, (USI))
DEF_RISCV_FTYPE (1, (VOID, USI))
DEF_RISCV_FTYPE (1, (ULONG, USI))
DEF_RISCV_FTYPE (1, (ULONG, ULONG))
DEF_RISCV_FTYPE (2, (VOID, ULONG, USI))
DEF_RISCV_FTYPE (2, (VOID, USI, USI))
DEF_RISCV_FTYPE (2, (SI, PSI, USI))
DEF_RISCV_FTYPE (2, (LLONG, PLLONG, USI))
DEF_RISCV_FTYPE (2, (ULONG, ULONG, USI))
DEF_RISCV_FTYPE (3, (SI, SI, PSI, USI))
DEF_RISCV_FTYPE (3, (USI, USI, PUSI, USI))
DEF_RISCV_FTYPE (3, (LLONG, LLONG, PLLONG, USI))
DEF_RISCV_FTYPE (3, (ULLONG, ULLONG, PULLONG, USI))
DEF_RISCV_FTYPE (1, (VOID, VOID))
DEF_RISCV_FTYPE (1, (ULONG, VOID))
DEF_RISCV_FTYPE (1, (VOID, ULONG))
DEF_RISCV_FTYPE (1, (LONG, LONG))
DEF_RISCV_FTYPE (2, (LONG, LONG, LONG))
DEF_RISCV_FTYPE (3, (LONG, LONG, LONG, LONG))
DEF_RISCV_FTYPE (4, (LONG, LONG, LONG, LONG, LONG))
DEF_RISCV_FTYPE (5, (LONG, LONG, LONG, LONG, LONG, LONG))
DEF_RISCV_FTYPE (6, (LONG, LONG, LONG, LONG, LONG, LONG, LONG))
DEF_RISCV_FTYPE (7, (LONG, LONG, LONG, LONG, LONG, LONG, LONG, LONG))
